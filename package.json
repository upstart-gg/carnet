{
  "name": "@upstart.gg/carnet",
  "version": "0.1.1",
  "description": "A framework for building AI agents with Vercel AI SDK using markdown-based agent definitions, skills, toolsets, and tools with smart prompt generation",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js"
    },
    "./schema/config.schema.json": "./schema/config.schema.json"
  },
  "bin": {
    "carnet": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "schema"
  ],
  "scripts": {
    "dev": "bun build --watch",
    "build": "bun build src/lib/index.ts src/cli/index.ts --outdir dist --target node",
    "test": "bun test",
    "lint": "biome lint src tests",
    "format": "biome format --write src",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "changeset": "changeset",
    "version:packages": "changeset version",
    "publish:packages": "changeset publish"
  },
  "keywords": [
    "ai",
    "agents",
    "prompts",
    "markdown",
    "cli",
    "llm",
    "tooling"
  ],
  "author": "Upstart <dev@upstart.gg> (https://upstart.gg)",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.0.0",
    "deno": ">=2.0.0"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "commander": "14.0.2",
    "gray-matter": "4.0.3"
  },
  "peerDependencies": {
    "ai": ">=5.0.0",
    "zod": ">=4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@types/bun": "latest",
    "ts-morph": "^27.0.2",
    "vitepress": "^1.6.4",
    "vitepress-plugin-tabs": "^0.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/upstart-gg/carnet.git"
  },
  "publishConfig": {
    "access": "public"
  }
}