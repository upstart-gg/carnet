{
  "name": "@upstart-gg/carnet",
  "version": "0.1.0",
  "description": "A library and CLI for managing AI agent definitions, skills, toolsets, and tools through markdown files with smart prompt generation",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js"
    },
    "./schema/config.schema.json": "./schema/config.schema.json"
  },
  "bin": {
    "carnet": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "schema"
  ],
  "scripts": {
    "dev": "bun build --watch",
    "build": "bun build src/lib/index.ts src/cli/index.ts --outdir dist --target node",
    "test": "bun test",
    "lint": "biome lint src",
    "format": "biome format --write src"
  },
  "keywords": [
    "ai",
    "agents",
    "prompts",
    "markdown",
    "cli",
    "llm",
    "tooling"
  ],
  "author": "Upstart",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.0.0",
    "deno": ">=2.0.0"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "commander": "14.0.2",
    "gray-matter": "4.0.3",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@types/bun": "latest"
  }
}