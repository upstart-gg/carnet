{
  "name": "@upstart-gg/carnet",
  "version": "0.1.0",
  "description": "A library and CLI for managing AI agent definitions, skills, toolsets, and tools through markdown files with smart prompt generation",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js"
    },
    "./schema/config.schema.json": "./schema/config.schema.json",
    "./adapters/vercel-ai": {
      "types": "./dist/adapters/vercel-ai/index.d.ts",
      "import": "./dist/adapters/vercel-ai/index.js"
    },
    "./adapters/openai": {
      "types": "./dist/adapters/openai/index.d.ts",
      "import": "./dist/adapters/openai/index.js"
    },
    "./adapters/anthropic": {
      "types": "./dist/adapters/anthropic/index.d.ts",
      "import": "./dist/adapters/anthropic/index.js"
    }
  },
  "bin": {
    "carnet": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "schema"
  ],
  "scripts": {
    "dev": "bun build --watch",
    "build": "bun build src/lib/index.ts src/cli/index.ts --outdir dist --target node",
    "test": "bun test",
    "lint": "biome lint src",
    "format": "biome format --write src",
    "docs:gen:cli": "bun run scripts/generate-cli-docs.ts",
    "docs:gen:schema": "bun run scripts/generate-schema-docs.ts",
    "docs:gen:manifest": "bun run scripts/generate-manifest-schema-docs.ts",
    "docs:dev": "bun run docs:gen:cli && bun run docs:gen:schema && bun run docs:gen:manifest && vitepress dev docs",
    "docs:build": "bun run docs:gen:cli && bun run docs:gen:schema && bun run docs:gen:manifest && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "changeset": "changeset",
    "version:packages": "changeset version",
    "publish:packages": "changeset publish"
  },
  "keywords": [
    "ai",
    "agents",
    "prompts",
    "markdown",
    "cli",
    "llm",
    "tooling"
  ],
  "author": "Upstart",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.0.0",
    "deno": ">=2.0.0"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "commander": "14.0.2",
    "gray-matter": "4.0.3"
  },
  "peerDependencies": {
    "ai": ">=5.0.1",
    "openai": ">=4.0.0",
    "@anthropic-ai/sdk": ">=0.20.0",
    "zod": ">=4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@types/bun": "latest",
    "ts-morph": "^27.0.2",
    "vitepress": "^1.6.4",
    "vitepress-plugin-tabs": "^0.7.3"
  }
}